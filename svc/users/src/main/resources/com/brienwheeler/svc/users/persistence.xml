		<class>com.brienwheeler.svc.users.domain.UserAttribute</class>
		<class>com.brienwheeler.svc.users.domain.UserEmailAddress</class>
		<class>com.brienwheeler.svc.users.domain.UserRole</class>
	</persistence-unit>

</persistence>
